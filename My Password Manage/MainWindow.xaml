        Title="My Password Manage" Height="250" Width="363">
    <Grid Margin="0,0">
        <Button x:Name="BtnChangePwd" Content="修改密码" HorizontalAlignment="Left" Margin="135,104,0,0" VerticalAlignment="Top" Width="75"/>
